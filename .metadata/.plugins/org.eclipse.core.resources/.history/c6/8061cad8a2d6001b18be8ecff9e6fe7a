package com.main;



public class Invoker {

	public static void main(String[] args) {
		
		
		
		System.out.println("First Car object which calls the parameterized constructor. ");
		Car carObj = new Car(4,"Blue","Toyota");
		carObj.drive();		
		carObj.park();
		
		
		System.out.println("\nSecond Car object which calls the no Varible-constructor and set the variables using getters and setters.");
		Car carObj2 = new Car();
		
		carObj2.setBrand("Tata MG Motor");
		carObj2.setColour("Maroon");
		carObj2.setNoOfwheels(4);
		
		carObj2.drive();
		carObj2.park();
	}

}
